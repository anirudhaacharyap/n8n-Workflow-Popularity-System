# Project Settings
PROJECT_NAME="n8n Workflow Popularity Tracker"
API_V1_STR="/api/v1"
ENV=dev

# Database (PostgreSQL)
# Format: postgresql+asyncpg://user:password@host:port/dbname
# For Docker Compose, host is 'db'. For local running without docker, use 'localhost'.
DATABASE_URL=postgresql+asyncpg://user:password@db/n8n_tracker

# Platform APIs
# Required for YouTube Data Collection
YOUTUBE_API_KEY=your_youtube_api_key_here

# Optional for public forum access, but recommended for higher limits
DISCOURSE_API_KEY=your_discourse_api_key_here

# Collection Settings
DAILY_QUOTA_YOUTUBE=10000
